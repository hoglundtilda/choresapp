    "apollo-server-express": "3.13.0",
    "apollo-server-plugin-base": "^3.7.2",
    "bcrypt": "^5.1.1",
    "cookie-session": "^2.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-pino-logger": "^7.0.0",
    "express-session": "^1.18.0",
