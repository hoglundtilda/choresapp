    "apollo-server-express": "3.11.1",
    "apollo-server-plugin-base": "^3.7.1",
    "bcrypt": "^5.1.0",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-pino-logger": "^7.0.0",
    "express-session": "^1.17.3",
    "graphql": "^16.6.0",
    "graphql-date-scalars": "^0.2.0",
    "jsonwebtoken": "^9.0.0",
    "passport": "^0.6.0",
